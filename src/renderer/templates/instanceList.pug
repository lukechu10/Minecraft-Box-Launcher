//- structure: InstanceSave[]
- if(data == undefined || data.length == 0)
	p= "You don't have any instances yet. Create one to start playing. ğŸ˜†"
- else
	- for(let i = 0; i < data.length; i++)
		section.item(data-instance-id=data[i].id
			data-instance-clienttype=data[i].clientType || "Vanilla"
			data-instance-type=data[i].type
			data-instance-name=data[i].name)
				.content
					.ui.grid
						.twelve.wide.column
							p.ui.header.text-instanceName= data[i].name
							p.description #{data[i].clientType || "Vanilla"} #[strong= data[i].id] (#{data[i].type})
						.four.wide.column
							.ui.right.floated.buttons
								- if(data[i].installed)
									.ui.green.button.btn-play(data-instance-name=data[i].name)
										| Play
								- else
									.ui.olive.button.btn-install(data-instance-name=data[i].name)
										| Install
								.ui.gray.floating.dropdown.button â–¼
									.menu
										.item.btn-rename(data-instance-name=data[i].name) Rename
										- if(data[i].installed)
											.item.btn-reinstall(data-instance-name=data[i].name class=data[i].installed) Reinstall
										- else
											.disabled.item Reinstall
										.item.btn-options(data-instance-name=data[i].name) Options
										.item.btn-saves(data-instance-name=data[i].name) Saves / Data
										.red.inverted.item.btn-delete(style="color: red;" data-instance-name=data[i].name) Delete